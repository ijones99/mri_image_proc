SUMMARY:
	LoadDicomFiles

	Used to load dicom files

USAGE:
        typedef itk::LoadDicomFiles<ImageTypeDoubleVol>  FilterTypeDicomLoad;
        FilterTypeDicomLoad::Pointer loadedFiles = FilterTypeDicomLoad::New();
        loadedFiles->SetBaseDirName(DIR_NAME);

        loadedFiles->SetSeriesDir1("1.3.12.2.1107.5.2.36.40270.2016120712544137360645849.0.0.0"); // mag 1 and 2
        loadedFiles->SetSeriesDir2("1.3.12.2.1107.5.2.36.40270.2016120712510398080545750.0.0.0"); // phase 1 and 2
        loadedFiles->SetSeriesDir3("1.3.12.2.1107.5.2.36.40270.2016120712581763361946644.0.0.0"); // mag 3 and 4
        loadedFiles->SetSeriesDir4("1.3.12.2.1107.5.2.36.40270.2016120712544929895746524.0.0.0"); // phase 3 and 4

        loadedFiles->Update();

        typename ImageTypeDoubleVol::Pointer mag1 = loadedFiles->GetOutputMag1();
        typename ImageTypeDoubleVol::Pointer mag2 = loadedFiles->GetOutputMag2();
        typename ImageTypeDoubleVol::Pointer mag3 = loadedFiles->GetOutputMag3();

        typename ImageTypeDoubleVol::Pointer phase1 = loadedFiles->GetOutputPhase1();
        typename ImageTypeDoubleVol::Pointer phase2 = loadedFiles->GetOutputPhase2();
        typename ImageTypeDoubleVol::Pointer phase3 = loadedFiles->GetOutputPhase3();
	
INPUT: 
	DIR_NAME: 
	SetSeriesDir(1...4): 

OUTPUT: 

	"wr" or "fr", depending on input order

